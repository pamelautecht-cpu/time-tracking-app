IsLoading] = useState(false);\n\n\tconst t = translations[language];\n\n\tuseEffect(() => {\n\t\tloadTimeCards();\n\t}, [user.id]);\n\n\tuseEffect(() => {\n\t\tlet interval: NodeJS.Timeout;\n\t\tif (activeShift && !isOnLunch) {\n\t\t\tinterval = setInterval(() => {\n\t\t\t\tconst clockInTime = parseInt(activeShift.clock_in_timestamp) * 1000;\n\t\t\t\tconst elapsed = Date.now() - clockInTime;\n\t\t\t\tsetElapsedTime(elapsed);\n\t\t\t}, 1000);\n\t\t}\n\t\treturn () => clearInterval(interval);\n\t}, [activeShift, isOnLunch]);\n\n\tconst loadTimeCards = async () => {\n\t\tconsole.log(\"Loading time cards for user:\", user.id);\n\t\tconst timeCardORM = TimeCardEntryORM.getInstance();\n\t\tconst userTimeCards = await timeCardORM.getTimeCardEntryByUserId(user.id);\n\n\t\tconsole.log(\"Found\", userTimeCards.length, \"time cards\");\n\n\t\t// Sort by clock in time (newest first)\n\t\tconst sorted = userTimeCards.sort((a, b) =>\n\t\t\tparseInt(b.clock_in_timestamp) - parseInt(a.clock_in_timestamp)\n\t\t);\n\n\t\tsetTimeCards(sorted);\n\n\t\t// Find active shift (no clock out time)\n\t\tconst active = sorted.find(tc => !tc.clock_out_timestamp);\n\t\tconsole.log(\"Active shift found:\", active ? `Yes (ID: ${active.id})` : \"No\");\n\n\t\tsetActiveShift(active || null);\n\t\tif (active) {\n\t\t\tsetLocation(active.location || \"\");\n\t\t\tsetWorkDescription(active.work_description || \"\");\n\t\t\t// Check if on lunch break\n\t\t\tconst onLunch = !!(active.lunch_start_timestamp && !active.lunch_end_timestamp);\n\t\t\tsetIsOnLunch(onLunch);\n\t\t\tconsole.log(\"On lunch:\", onLunch);\n\t\t} else {\n\t\t\tsetIsOnLunch(false);\n\t\t}\n\t};\n\n\tconst handleClockIn = async () => {\n\t\tconsole.log(\"=== CLOCK IN STARTED ===\");\n\t\tconsole.log(\"User ID:\", user.id);\n\t\tconsole.log(\"Location:\", location);\n\t\tconsole.log(\"Work Description:\", workDescription);\n\n\t\tsetIsLoading(true);\n\t\ttry {\n\t\t\tconst timeCardORM = TimeCardEntryORM.getInstance();\n\t\t\tconst now = Math.floor(Date.now() / 1000).toString();\n\t\t\tconsole.log(\"Current timestamp:\", now);\n\n\t\t\tconst newEntry = {\n\t\t\t\tuser_id: user.id,\n\t\t\t\tclock_in_timestamp: now,\n\t\t\t\tlocation: location || null,\n\t\t\t\twork_description: workDescription || null,\n\t\t\t} as TimeCardEntryModel;\n\n\t\t\tconsole.log(\"Inserting time card entry:\", newEntry);\n\n\t\t\tconst result = await timeCardORM.insertTimeCardEntry([newEntry]);\n\t\t\tconsole.log(\"Insert result:\", result);\n\n\t\t\tconsole.log(\"Time card entry created successfully!\");\n\n\t\t\t// Clear form\n\t\t\tsetLocation(\"\");\n\t\t\tsetWorkDescription(\"\");\n\n\t\t\t// Reload time cards\n\t\t\tconsole.log(\"Reloading time cards...\");\n\t\t\tawait loadTimeCards();\n\n\t\t\tconsole.log(\"=== CLOCK IN COMPLETED ===\");\n\n\t\t\t// Show success feedback\n\t\t\tif (activeShift) {\n\t\t\t\talert(\"‚úÖ Successfully clocked in! You can now see the CLOCK OUT button.\");\n\t\t\t}\n\t\t} catch (error) {\n\t\t\tconsole.error(\"=== CLOCK IN FAILED ===\");\n\t\t\tconsole.error(\"Error details:\", error);\n\t\t\talert(`Error clocking in: ${error instanceof Error ? error.message : 'Unknown error'}. Check console for details.`);\n\t\t} finally {\n\t\t\tsetIsLoading(false);\n\t\t}\n\t};\n\n\tconst handleClockOut = async () => {\n\t\tconsole.log(\"Clock Out clicked - activeShift:\", activeShift);\n\n\t\tif (!activeShift) {\n\t\t\tconsole.error(\"No active shift to clock out!\");\n\t\t\talert(\"Error: No active shift found. Please try clocking in first.\");\n\t\t\treturn;\n\t\t}\n\n\t\ttry {\n\t\t\tconst timeCardORM = TimeCardEntryORM.getInstance();\n\t\t\tconst now = Math.floor(Date.now() / 1000).toString();\n\n\t\t\tconst clockInTime = parseInt(activeShift.clock_in_timestamp);\n\t\t\tconst clockOutTime = parseInt(now);\n\t\t\tlet totalHours = (clockOutTime - clockInTime) / 3600;\n\n\t\t\t// Deduct lunch time if lunch was taken\n\t\t\tif (activeShift.lunch_duration_minutes) {\n\t\t\t\ttotalHours -= activeShift.lunch_duration_minutes / 60;\n\t\t\t}\n\n\t\t\tconsole.log(\"Clocking out - Total hours:\", totalHours);\n\n\t\t\tawait timeCardORM.setTimeCardEntryById(activeShift.id, {\n\t\t\t\t...activeShift,\n\t\t\t\tclock_out_timestamp: now,\n\t\t\t\ttotal_hours: totalHours,\n\t\t\t\tlocation: location || activeShift.location,\n\t\t\t\twork_description: workDescription || activeShift.work_description,\n\t\t\t});\n\n\t\t\tconsole.log(\"Clock out successful!\");\n\t\t\tsetLocation(\"\");\n\t\t\tsetWorkDescription(\"\");\n\t\t\tawait loadTimeCards();\n\t\t} catch (error) {\n\t\t\tconsole.error(\"Error clocking out:\", error);\n\t\t\talert(\"Error clocking out. Please try again.\");\n\t\t}\n\t};\n\n\tconst handleStartLunch = async () => {\n\t\tif (!activeShift) return;\n\n\t\tconst timeCardORM = TimeCardEntryORM.getInstance();\n\t\tconst now = Math.floor(Date.now() / 1000).toString();\n\n\t\tawait timeCardORM.setTimeCardEntryById(activeShift.id, {\n\t\t\t...activeShift,\n\t\t\tlunch_start_timestamp: now,\n\t\t\tlocation: location || activeShift.location,\n\t\t\twork_description: workDescription || activeShift.work_description,\n\t\t});\n\n\t\tawait loadTimeCards();\n\t};\n\n\tconst handleEndLunch = async () => {\n\t\tif (!activeShift || !activeShift.lunch_start_timestamp) return;\n\n\t\tconst timeCardORM = TimeCardEntryORM.getInstance();\n\t\tconst now = Math.floor(Date.now() / 1000).toString();\n\n\t\tconst lunchStartTime = parseInt(activeShift.lunch_start_timestamp);\n\t\tconst lunchEndTime = parseInt(now);\n\t\tconst lunchDurationMinutes = (lunchEndTime - lunchStartTime) / 60;\n\n\t\t// Add to existing lunch duration if there was already one\n\t\tconst totalLunchMinutes = (activeShift.lunch_duration_minutes || 0) + lunchDurationMinutes;\n\n\t\tawait timeCardORM.setTimeCardEntryById(activeShift.id, {\n\t\t\t...activeShift,\n\t\t\tlunch_end_timestamp: now,\n\t\t\tlunch_duration_minutes: totalLunchMinutes,\n\t\t});\n\n\t\tawait loadTimeCards();\n\t};\n\n\tconst formatTime = (ms: num