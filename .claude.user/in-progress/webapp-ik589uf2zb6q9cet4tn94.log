lParams.get('employee');\n\t\t\tconst employeeEmail = urlParams.get('email');\n\t\t\tconst employeePhone = urlParams.get('phone');\n\n\t\t\tif (employeeId || employeeEmail || employeePhone) {\n\t\t\t\tconsole.log('=== AUTO-LOGIN ATTEMPT ===');\n\t\t\t\tif (employeeId) console.log('Employee ID from URL:', employeeId);\n\t\t\t\tif (employeeEmail) console.log('Employee Email from URL:', employeeEmail);\n\t\t\t\tif (employeePhone) console.log('Employee Phone from URL:', employeePhone);\n\t\t\t\tconsole.log('Available users:', users.map(u => ({ id: u.id, name: u.name, email: u.email, phone: u.phone, role: u.role })));\n\n\t\t\t\t// Try to find by ID first, then by phone (most reliable), then by email as fallbacks\n\t\t\t\tlet user = employeeId ? users.find(u => u.id === employeeId) : null;\n\n\t\t\t\tif (!user && employeePhone) {\n\t\t\t\t\tuser = users.find(u => u.phone === employeePhone && u.role === UserRole.Employee);\n\t\t\t\t\tconsole.log('Attempted phone-based lookup for:', employeePhone, 'Found:', !!user);\n\t\t\t\t}\n\n\t\t\t\tif (!user && employeeEmail) {\n\t\t\t\t\tuser = users.find(u => u.email === employeeEmail && u.role === UserRole.Employee);\n\t\t\t\t\tconsole.log('Attempted email-based lookup for:', employeeEmail, 'Found:', !!user);\n\t\t\t\t}\n\n\t\t\t\tif (user && user.role === UserRole.Employee) {\n\t\t\t\t\tconsole.log('✓ Auto-login successful for:', user.name);\n\t\t\t\t\tconsole.log('Setting currentUser to:', user);\n\t\t\t\t\tconsole.log('Hiding user select screen');\n\t\t\t\t\tsetCurrentUser(user);\n\t\t\t\t\tsetShowUserSelect(false);\n\t\t\t\t\tsetEmployeeLinkMode(user.id); // Store employee ID to restrict access\n\t\t\t\t\tsetIsAutoLoggingIn(false);\n\t\t\t\t\t// Clean up URL\n\t\t\t\t\twindow.history.replaceState({}, '', window.location.pathname);\n\t\t\t\t\tconsole.log('✓ Auto-login complete - should now show employee time tracking view');\n\t\t\t\t} else if (user) {\n\t\t\t\t\tconsole.warn('✗ User found but not an employee (role:', user.role