omORM(\n\t\t\t\tawait timeCardORM.getTimeCardEntryByUserId(user.id)\n\t\t\t);\n\t\t\tif (reloadedShifts.some(tc => isActiveShift(tc))) {\n\t\t\t\talert(\"✅ Successfully clocked in! You can now see the CLOCK OUT button.\");\n\t\t\t}\n\t\t} catch (error) {\n\t\t\tconsole.error(\"=== CLOCK IN FAILED ===\");\n\t\t\tconsole.error(\"Error details:\", error);\n\t\t\talert(`Error clocking in: ${error instanceof Error ? error.message : 'Unknown error'}. Check console for details.`);\n\t\t} finally {\n\t\t\tsetIsLoading(false);\n\t\t}\n\t};\n\n\tconst handleClockOut = async () => {\n\t\tconsole.log(\"Clock Out clicked - activeShift:\", activeShift);\n\n\t\tif (!activeShift) {\n\t\t\tconsole.error(\"No active shift to clock out!\");\n\t\t\talert(\"Error: No active shift found. Please try clocking in first.\");\n\t\t\treturn;\n\t\t}\n\n\t\ttry {\n\t\t\tconst timeCardORM = TimeCardEntryORM.getInstance();\n\t\t\tconst now = Math.floor(Date.now() / 1000).toString();\n\n\t\t\tconst clockInTime = parseInt(activeShift.clock_in_timestamp);\n\t\t\tconst clockOutTime = parseInt(now);\n\t\t\tlet totalHours = (clockOutTime - clockInTime) / 3600;\n\n\t\t\t// Deduct lunch time if lunch was taken\n\t\t\tif (activeShift.lunch_duration_minutes && activeShift.lunch_duration_minutes > 0) {\n\t\t\t\ttotalHours -= activeShift.lunch_duration_minutes / 60;\n\t\t\t}\n\n\t\t\tconsole.log(\"Clocking out - Total hours:\", totalHours);\n\n\t\t\tconst updateData = {\n\t\t\t\t...activeShift,\n\t\t\t\tclock_out_timestamp: now,\n\t\t\t\ttotal_hours: totalHours,\n\t\t\t\tlocation: location || activeShift.location || undefined,\n\t\t\t\twork_description: workDescription || activeShift.work_description || undefined,\n\t\t\t};\n\n\t\t\t// Normalize before saving\n\t\t\tconst normalizedUpdate = normalizeTimeCardEntryForSave(updateData as TimeCardEntryModel);\n\n\t\t\tawait timeCardORM.setTimeCardEntryById(activeShift.id, normalizedUpdate as TimeCardEntryModel);\n\n\t\t\tconsole.log(\"Clock out successful!\");\n\t