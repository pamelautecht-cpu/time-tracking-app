\t}\n\t}, [users]);\n\n\tconst loadUsers = () => {\n\t\ttry {\n\t\t\tconsole.log('Loading users from localStorage...');\n\n\t\t\t// Try to load from localStorage first\n\t\t\tconst stored = localStorage.getItem('timetracking_users');\n\t\t\tif (stored) {\n\t\t\t\tconst parsedUsers = JSON.parse(stored) as UserModel[];\n\t\t\t\tconsole.log('Users loaded from localStorage:', parsedUsers.length);\n\t\t\t\tsetUsers(parsedUsers);\n\t\t\t\treturn;\n\t\t\t}\n\n\t\t\t// If no stored users, create demo users immediately\n\t\t\tconsole.log('No users found, creating demo users...');\n\t\t\tconst now = new Date().toISOString();\n\t\t\tconst demoUsers: UserModel[] = [\n\t\t\t\t{\n\t\t\t\t\tid: crypto.randomUUID(),\n\t\t\t\t\tdata_creator: 'system',\n\t\t\t\t\tdata_updater: 'system',\n\t\t\t\t\tcreate_time: now,\n\t\t\t\t\tupdate_time: now,\n\t\t\t\t\tname: \"John Smith\",\n\t\t\t\t\temail: \"john@example.com\",\n\t\t\t\t\trole: UserRole.Employee,\n\t\t\t\t\thourly_rate: 15.00,\n\t\t\t\t},\n\t\t\t\t{\n\t\t\t\t\tid: crypto.randomUUID(),\n\t\t\t\t\tdata_creator: 'system',\n\t\t\t\t\tdata_updater: 'system',\n\t\t\t\t\tcreate_time: now,\n\t\t\t\t\tupdate_time: now,\n\t\t\t\t\tname: \"Sarah Johnson\",\n\t\t\t\t\temail: \"sarah@example.com\",\n\t\t\t\t\trole: UserRole.Employee,\n\t\t\t\t\thourly_rate: 18.00,\n\t\t\t\t},\n\t\t\t\t{\n\t\t\t\t\tid: crypto.randomUUID(),\n\t\t\t\t\tdata_creator: 'system',\n\t\t\t\t\tdata_updater: 'system',\n\t\t\t\t\tcreate_time: now,\n\t\t\t\t\tupdate_time: now,\n\t\t\t\t\tname: \"Mike Manager\",\n\t\t\t\t\temail: \"mike@example.com\",\n\t\t\t\t\trole: UserRole.Manager,\n\t\t\t\t\thourly_rate: 25.00,\n\t\t\t\t},\n\t\t\t];\n\n\t\t\t// Save to localStorage\n\t\t\tlocalStorage.setItem('timetracking_users', JSON.stringify(demoUsers));\n\t\t\tconsole.log('Demo users created:', demoUsers.length);\n\t\t\tsetUsers(demoUsers);\n\t\t} catch (error) {\n\t\t\tconsole.error('Error loading users:', error);\n\t\t\tsetUsers([]);\n\t\t}\n\t};\n\n\tconst handleUserSelect = (