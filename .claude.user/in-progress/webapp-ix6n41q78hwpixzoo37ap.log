"Enviar Enlace por Correo\",\n\t\tmyTimeTracking: \"Mi Registro de Tiempo\",\n\t\tmyHours: \"Mis Horas\",\n\t}\n};\n\nfunction App() {\n\t// Get employee ID from URL using vanilla JavaScript (more reliable)\n\tconst getEmployeeIdFromUrl = () => {\n\t\tif (typeof window !== 'undefined') {\n\t\t\tconst params = new URLSearchParams(window.location.search);\n\t\t\treturn params.get('employee');\n\t\t}\n\t\treturn null;\n\t};\n\n\tconst [currentUser, setCurrentUser] = useState<UserModel | null>(null);\n\tconst [users, setUsers] = useState<UserModel[]>([]);\n\tconst [showUserSelect, setShowUserSelect] = useState(true);\n\tconst [language, setLanguage] = useState<Language>('en');\n\tconst [employeeOnlyMode, setEmployeeOnlyMode] = useState(false);\n\tconst [employeeIdFromUrl] = useState<string | null>(getEmployeeIdFromUrl());\n\tconst [loadingTimeout, setLoadingTimeout] = useState(false);\n\n\tconst t = translations[language];\n\n\tuseEffect(() => {\n\t\tloadUsers();\n\t}, []);\n\n\t// Add a timeout for loading\n\tuseEffect(() => {\n\t\tif (employeeOnlyMode && !currentUser) {\n\t\t\tconst timer = setTimeout(() => {\n\t\t\t\tconsole.warn('Loading timeout - users took too long to load');\n\t\t\t\tsetLoadingTimeout(true);\n\t\t\t}, 10000); // 10 second timeout\n\n\t\t\treturn () => clearTimeout(timer);\n\t\t}\n\t}, [employeeOnlyMode, currentUser]);\n\n\t// Update UI state when employee ID is detected from URL\n\tuseEffect(() => {\n\t\tif (employeeIdFromUrl) {\n\t\t\tconsole.log('Employee ID detected from URL:', employeeIdFromUrl);\n\t\t\tsetShowUserSelect(false);\n\t\t\tsetEmployeeOnlyMode(true);\n\t\t}\n\t}, [employeeIdFromUrl]);\n\n\t// Check for employee URL parameter and auto-login\n\tuseEffect(() => {\n\t\tif (users.length > 0 && !currentUser && employeeIdFromUrl) {\n\t\t\tconsole.log('Attempting auto-login for employee:', employeeIdFromUrl);\n\t\t\tconsole.log('Available users:', users.map(u => ({ id: u.id, name: u.name, role: u.role })));\n\n\t\t\tconst user = users.find(u => u.id === employeeIdFromUrl);\n\t\t\tconsole.log('Found user:', user);\n\n\t\t\tif (user && user.role === UserRole.Employee) {\n\t\t\t\tconsole.log('Auto-login successful for:', user.name);\n\t\t\t\tsetCurrentUser(user);\n\t\t\t\tsetShowUserSelect(false);\n\t\t\t\tsetEmployeeOnlyMode(true);\n\t\t\t} else if (user) {\n\t\t\t\tconsole.warn('User found but not an Employee role:', user.role);\n\t\t\t} else {\n\t\t\t\tconsole.error('No user found with ID:', employeeIdFromUrl);\n\t\t\t}\n\t\t}\n\t}, [users, employeeIdFromUrl, currentUser]);\n\n\tconst loadUsers = async () => {\n\t\ttry {\n\t\t\tconsole.log('Loading users from database...');\n\t\t\tconst userORM = UserORM.getInstance();\n\t\t\tconst allUsers = await userORM.getAllUser();\n\n\t\t\tconsole.log('Users loaded:', allUsers.length);\n\n\t\t\t// If no users exist, create demo users\n\t\t\tif (allUsers.length === 0) {\n\t\t\t\tconsole.log('No users found, creating demo users...');\n\t\t\t\tconst demoUsers = await userORM.insertUser([\n\t\t\t\t\t{\n\t\t\t\t\t\tname: \"John Smith\",\n\t\t\t\t\t\temail: \"john@example.com\",\n\t\t\t\t\t\trole: UserRole.Employee,\n\t\t\t\t\t} as UserModel,\n\t\t\t\t\t{\n\t\t\t\t\t\tname: \"Sarah Johnson\",\n\t\t\t\t\t\temail: \"sarah@example.com\",\n\t\t\t\t\t\trole: UserRole.Employee,\n\t\t\t\t\t} as UserModel,\n\t\t\t\t\t{\n\t\t\t\t\t\tname: \"Mike Manager\",\n\t\t\t\t\t\temail: \"mike@example.com\",\n\t\t\t\t\t\trole: UserRole.Manager,\n\t\t\t\t\t} as UserModel,\n\t\t\t\t]);\n\t\t\t\tconsole.log('Demo users created:', demoUsers.length);\n\t\t\t\tsetUsers(demoUsers);\n\t\t\t} else {\n\t\t\t\tconsole.log('Setting users:', allUsers.map(u => ({ id: u.id, name: u.name, role: u.role })));\n\t\t\t\tsetUsers(allUsers);\n\t\t\t}\n\t\t} catch (error) {\n\t\t\tconsole.error('Error loading users:', error);\n\t\t\t// Still set empty array to prevent infinite loading\n\t\t\tsetUsers([]);\n\t\t}\n\t};\n\n\tconst handleUserSelect = (userId: string) => {\n\t\tconst user = users.find(u => u.id 