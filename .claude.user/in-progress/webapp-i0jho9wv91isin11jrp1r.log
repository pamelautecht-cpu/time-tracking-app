EMPT ===');\n\t\t\t\tif (employeeId) console.log('Employee ID from URL:', employeeId);\n\t\t\t\tif (employeeEmail) console.log('Employee Email from URL:', employeeEmail);\n\t\t\t\tconsole.log('Available users:', users.map(u => ({ id: u.id, name: u.name, email: u.email, role: u.role })));\n\n\t\t\t\t// Try to find by ID first, then by email as fallback\n\t\t\t\tlet user = employeeId ? users.find(u => u.id === employeeId) : null;\n\t\t\t\tif (!user && employeeEmail) {\n\t\t\t\t\tuser = users.find(u => u.email === employeeEmail && u.role === UserRole.Employee);\n\t\t\t\t\tconsole.log('Attempted email-based lookup for:', employeeEmail, 'Found:', !!user);\n\t\t\t\t}\n\n\t\t\t\tif (user && user.role === UserRole.Employee) {\n\t\t\t\t\tconsole.log('✓ Auto-login successful for:', user.name);\n\t\t\t\t\tsetCurrentUser(user);\n\t\t\t\t\tsetShowUserSelect(false);\n\t\t\t\t\tsetEmployeeLinkMode(user.id); // Store employee ID to restrict access\n\t\t\t\t\t// Clean up URL\n\t\t\t\t\twindow.history.replaceState({}, '', window.location.pathname);\n\t\t\t\t} else if (user) {\n\t\t\t\t\tconsole.warn('✗ User found but not an employee (role:', user.role, ')');\n\t\t\t\t\tsetEmployeeLinkMode(employeeId || 'invalid');\n\t\t\t\t} else {\n\t\t\t\t\tconsole.error('✗ Employee not found in database');\n\t\t\t\t\tconsole.log('This usually means the localStorage was cleared or the employee was deleted.');\n\t\t\t\t\tconsole.log('Available employees:', users.filter(u => u.role === UserRole.Employee).map(u => ({ id: u.id, email: u.email })));\n\t\t\t\t\t// Still set employee link mode so they can only see employees\n\t\t\t\t\tsetEmployeeLinkMode(employeeId || 'invalid');\n\t\t\t\t}\n\t\t\t}\n\t\t}\n\t}, [users]);\n\n\tconst loadUsers = () => {\n\t\ttry {\n\t\t\tconsole.log('Loading users from localStorage...');\n\n\t\t\t// Try to load from localStorage first\n\t\t\tconst stored = localStorage.getItem('timetracking_users');\n\t\t\tif (stored) {\n\t\t\t\tconst parsedUsers = JSON.parse(stored) as UserModel[];\n\t\t\t\tconsole.log('Users loaded from localStorage:', parsedUsers.length);\n\t\t\t\tsetUsers(parsedUsers);\n\t\t\t\treturn;\n\t\t\t}\n\n\t\t\t// If no stored users, create demo users immediately\n\t\t\tconsole.log('No users found, creating demo users...');\n\t\t\tconst now = new Date().toISOString();\n\t\t\tconst demoUsers: UserModel[] = [\n\t\t\t\t{\n\t\t\t\t\tid: crypto.randomUUID(),\n\t\t\t\t\tdata_creator: 'system',\n\t\t\t\t\tdata_updater: 'system',\n\t\t\t\t\tcreate_time: now,\n\t\t\t\t\tupdate_time: now,\n\t\t\t\t\tname: \"John Smith\",\n\t\t\t\t\temail: \"john@example.com\",\n\t\t\t\t\trole: UserRole.Employee,\n\t\t\t\t\thourly_rate: 15.00,\n\t\t\t\t},\n\t\t\t\t{\n\t\t\t\t\tid: crypto.randomUUID(),\n\t\t\t\t\tdata_creator: 'system',\n\t\t\t\t\tdata_updater: 'system',\n\t\t\t\t\tcreate_time: now,\n\t\t\t\t\tupdate_time: now,\n\t\t\t\t\tname: \"Sarah Johnson\",\n\t\t\t\t\temail: \"sarah@example.com\",\n\t\t\t\t\trole: UserRole.Employee,\n\t\t\t\t\thourly_rate: 18.00,\n\t\t\t\t},\n\t\t\t\t{\n\t\t\t\t\tid: crypto.randomUUID(),\n\t\t\t\t\tdata_creator: 'system',\n\t\t\t\t\tdata_updater: 'system',\n\t\t\t\t\tcreate_time: now,\n\t\t\t\t\tupdate_time: now,\n\t\t\t\t\tname: \"Mike Manager\",\n\t\t\t\t\temail: \"mike@example.com\",\n\t\t\t\t\trole: UserRole.Manager,\n\t\t\t\t\thourly_rate: 25.00,\n\t\t\t\t},\n\t\t\t];\n\n\t\t\t// Save to localStorage\n\t\t\tlocalStorage.setItem('timetracking_users', JSON.stringify(demoUsers));\n\t\t\tconsole.log('Demo users created:', demoUsers.length);\n\t\t\tsetUsers(demoUsers);\n\t\t} catch (error) {\n\t\t\tconsole.error('Error loading users:', error);\n\t\t\tsetUsers([]);\n\t\t}\n\t};\n\n\tconst handleUserSelect = (userId: string) => {\n\t\tconsole.log('User selected:', userId);\n\t\tconsole.log('Available users:', users);\n\t\tconst user = users.find(u => u.id === userId);\n\t\tconsole.log('Found user:', user);\n\t\tif (user) {\n\t\t\tsetCurrentUser(user);\n\t\t\tsetShowUserSelect(false);\n\t\t\tconsole.log('User set