ctiveShift,\n\t\t\tlunch_start_timestamp: now,\n\t\t\tlocation: location || activeShift.location || undefined,\n\t\t\twork_description: workDescription || activeShift.work_description || undefined,\n\t\t};\n\n\t\tconst normalizedUpdate = normalizeTimeCardEntryForSave(updateData as TimeCardEntryModel);\n\t\tawait timeCardORM.setTimeCardEntryById(activeShift.id, normalizedUpdate as TimeCardEntryModel);\n\n\t\tawait loadTimeCards();\n\t};\n\n\tconst handleEndLunch = async () => {\n\t\tif (!activeShift || !activeShift.lunch_start_timestamp) return;\n\n\t\tconst timeCardORM = TimeCardEntryORM.getInstance();\n\t\tconst now = Math.floor(Date.now() / 1000).toString();\n\n\t\t// Handle case where lunch_start_timestamp might be the string \"null\"\n\t\tlet lunchStartTime: number;\n\t\tif (typeof activeShift.lunch_start_timestamp === 'string' && activeShift.lunch_start_timestamp !== 'null') {\n\t\t\tlunchStartTime = parseInt(activeShift.lunch_start_timestamp);\n\t\t} else {\n\t\t\treturn;\n\t\t}\n\n\t\tconst lunchEndTime = parseInt(now);\n\t\tconst lunchDurationMinutes = (lunchEndTime - lunchStartTime) / 60;\n\n\t\t// Add to existing lunch duration if there was already one\n\t\tconst totalLunchMinutes = (activeShift.lunch_duration_minutes && activeShift.lunch_duration_minutes > 0 ? activeShift.lunch_duration_minutes : 0) + lunchDurationMinutes;\n\n\t\tconst updateData = {\n\t\t\t...activeShift,\n\t\t\tlunch_end_timestamp: now,\n\t\t\tlunch_duration_minutes: totalLunchMinutes,\n\t\t};\n\n\t\tconst normalizedUpdate = normalizeTimeCardEntryForSave(updateData as TimeCardEntryModel);\n\t\tawait timeCardORM.setTimeCardEntryById(activeShift.id, normalizedUpdate as TimeCardEntryModel);\n\n\t\tawait loadTimeCards();\n\t};\n\n\tconst formatTime = (ms: number) => {\n\t\tconst seconds = Math.floor(ms / 1000);\n\t\tconst hours = Math.floor(seconds / 3600);\n\t\tconst minutes = Math.floor((seconds % 3600) / 60);\n\t\tconst secs = seconds % 60;\n\t\treturn `${hours.toString().padStart(2, '0'