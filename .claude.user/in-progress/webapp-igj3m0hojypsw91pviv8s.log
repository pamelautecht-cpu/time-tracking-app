 (!user && employeeEmail) {\n\t\t\t\t\tuser = users.find(u => u.email === employeeEmail && u.role === UserRole.Employee);\n\t\t\t\t\tconsole.log('Attempted email-based lookup for:', employeeEmail, 'Found:', !!user);\n\t\t\t\t}\n\n\t\t\t\tif (user && user.role === UserRole.Employee) {\n\t\t\t\t\tconsole.log('✓ Auto-login successful for:', user.name);\n\t\t\t\t\tconsole.log('Setting currentUser to:', user);\n\t\t\t\t\tconsole.log('Hiding user select screen');\n\t\t\t\t\tsetCurrentUser(user);\n\t\t\t\t\tsetShowUserSelect(false);\n\t\t\t\t\tsetEmployeeLinkMode(user.id); // Store employee ID to restrict access\n\t\t\t\t\tsetIsAutoLoggingIn(false);\n\t\t\t\t\t// Clean up URL\n\t\t\t\t\twindow.history.replaceState({}, '', window.location.pathname);\n\t\t\t\t\tconsole.log('✓ Auto-login complete - should now show employee time tracking view');\n\t\t\t\t} else if (user) {\n\t\t\t\t\tconsole.warn('✗ User found but not an employee (role:', user.role, ')');\n\t\t\t\t\tsetEmployeeLinkMode(employeeId || 'invalid');\n\t\t\t\t\tsetIsAutoLoggingIn(false);\n\t\t\t\t} else {\n\t\t\t\t\tconsole.error('✗ Employee not found in database');\n\t\t\t\t\tconsole.log('This usually means the localStorage was cleared or the employee was deleted.');\n\t\t\t\t\tconsole.log('Tried to find by ID:', employeeId);\n\t\t\t\t\tconsole.log('Tried to find by email:', employeeEmail);\n\t\t\t\t\tconsole.log('Available employees:', users.filter(u => u.role === UserRole.Employee).map(u => ({ id: u.id, name: u.name, email: u.email })));\n\t\t\t\t\t// Still set employee link mode so they can only see employees\n\t\t\t\t\tsetEmployeeLinkMode(employeeId || 'invalid');\n\t\t\t\t\tsetIsAutoLoggingIn(false);\n\t\t\t\t}\n\t\t\t}\n\t\t}\n\t}, [users]);\n\n\tconst loadUsers = () => {\n\t\ttry {\n\t\t\tconsole.log('Loading users from localStorage...');\n\n\t\t\t// Try to load from localStorage first\n\t\t\tconst stored = localStorage.getItem('timetracking_users');\n\t\t\tif (stored) {\n\t\t\t\tconst parsedUsers = JSON.parse(stored) as UserModel[];\n\t\t\t\tconsole.log('Users loaded from localStorage:', parsedUsers.length);\n\t\t\t\tsetUsers(parsedUsers);\n\t\t\t\treturn;\n\t\t\t}\n\n\t\t\t// If no stored users, create demo users immediately\n\t\t\tconsole.log('No users found, creating demo users...');\n\t\t\tconst now = new Date().toISOString();\n\t\t\tconst demoUsers: UserModel[] = [\n\t\t\t\t{\n\t\t\t\t\tid: crypto.randomUUID(),\n\t\t\t\t\tdata_creator: 'system',\n\t\t\t\t\tdata_updater: 'system',\n\t\t\t\t\tcreate_time: now,\n\t\t\t\t\tupdate_time: now,\n\t\t\t\t\tname: \"John Smith\",\n\t\t\t\t\temail: \"john@example.com\",\n\t\t\t\t\trole: UserRole.Employee,\n\t\t\t\t\thourly_rate: 15.00,\n\t\t\t\t},\n\t\t\t\t{\n\t\t\t\t\tid: crypto.randomUUID(),\n\t\t\t\t\tdata_creator: 'system',\n\t\t\t\t\tdata_updater: 'system',\n\t\t\t\t\tcreate_time: now,\n\t\t\t\t\tupdate_time: now,\n\t\t\t\t\tname: \"Sarah Johnson\",\n\t\t\t\t\temail: \"sarah@example.com\",\n\t\t\t\t\trole: UserRole.Employee,\n\t\t\t\t\thourly_rate: 18.00,\n\t\t\t\t},\n\t\t\t\t{\n\t\t\t\t\tid: crypto.randomUUID(),\n\t\t\t\t\tdata_creator: 'system',\n\t\t\t\t\tdata_updater: 'system',\n\t\t\t\t\tcreate_time: now,\n\t\t\t\t\tupdate_time: now,\n\t\t\t\t\tname: \"Mike Manager\",\n\t\t\t\t\temail: \"mike@example.com\",\n\t\t\t\t\trole: UserRole.Manager,\n\t\t\t\t\thourly_rate: 25.00,\n\t\t\t\t},\n\t\t\t];\n\n\t\t\t// Save to localStorage\n\t\t\tlocalStorage.setItem('timetracking_users', JSON.stringify(demoUsers));\n\t\t\tconsole.log('Demo users created:', demoUsers.length);\n\t\t\tsetUsers(demoUsers);\n\t\t} catch (error) {\n\t\t\tconsole.error('Error loading users:', error);\n\t\t\tsetUsers([]);\n\t\t}\n\t};\n\n\tconst handleUserSelect = (userId: string) => {\n\t\tconsole.log('User selected:', userId);\n\t\tconsole.log('Available users:', users);\n\t\tconst user = users.find(u => u.id === userId);\n\t\tconsole.log('Found user:', user);\n\t\tif (user) {\n\t\t\tsetCurrentUser(user);\n\t\t\tsetShowUserSelect(false);\n\t\t\tconsole.log('User set successfully:', user.name);\n\t\t} else {\n\t\t\tconsole.error('User not found in users array');\n\t\t}\n\t};\n\n\tconst handleLogout = () => {\n\t\tsetCurrentUser(null);\n\t\tsetShowUserSelect(true);\n\t};\n\n\tconst toggleLanguage = () => {\n\t\tsetLanguage(prev => prev === 'en' ? 'es' : 'en');\n\t};\n\n\t// Show user select screen if no current user\n\tif (showUserSelect || !currentUser) {\n\t\t// Filter users based on employee link mode\n\t\tconst availableUsers = employeeLinkMode\n\t\t\t? users.filter(u => u.id === employeeLinkMode) // Only show the specific employee\n\t\t\t: users; // Show all users for normal access\n\n\t\t// Check if employee link is broken (mode is set but no matching user found)\n\t\tconst isEmployeeLinkBroken = employeeLinkMode && users.length > 0 && availableUsers.length === 0;\n\n\t\treturn (\n\t\t\t<div className=\"min-h-screen bg-gradient-to-br from-blue-50 to-indigo-100 dark:from-gray-900 dark:to-gray-800 flex items-center justify-center p-4\">\n\t\t\t\t<div className=\"absolute top-4 right-4\">\n\t\t\t\t\t<Button onClick={toggleLanguage} variant=\"outline\" size=\"sm\">\n\t\t\t\t\t\t<Languages className=\"w-4 h-4 mr-2\" />\n\t\t\t\t\t\t{language === 'en' ? 'Español' : 'English'}\n\t\t\t\t\t</Button>\n\t\t\t\t</div>\n\t\t\t\t<Card className=\"w-full max-w-md\">\n\t\t\t\t\t<CardHeader>\n\t\t\t\t\t\t<CardTitle className=\"text-2xl\">{t.appTitle}</CardTitle>\n\t\t\t\t\t\t<CardDescription>{employeeLinkMode ? t.myTimeTracking : t.selectAccount}</CardDescription>\n\t\t\t\t\t</CardHeader>\n\t\t\t\t\t<CardContent className=\"space-y-4\">\n\t\t\t\t\t\t{isEmployeeLinkBroken ? (\n\t\t\t\t\t\t\t<div className=\"text-center py-8 space-y-4\">\n\t\t\t\t\t\t\t\t<AlertCircle className=\"w-12 h-12 mx-auto text-amber-500\" />\n\t\t\t\t\t\t\t\t<div>\n\t\t\t\t\t\t\t\t\t<p className=\"font-semibold text-foreground mb-2\">\n\t\t\t\t\t\t\t\t\t\t{language === 'en' ? 'Link Expired' : 'Enlace Expirado'}\n\t\t\t\t\t\t\t\t\t</p>\n\t\t\t\t\t\t\t\