ers();\n\t}, [allSickDays, selectedEmployee, dateFilter]);\n\n\tconst loadAllSickDays = async () => {\n\t\tconst sickDayORM = SickDayORM.getInstance();\n\t\tconst allDays = await sickDayORM.getAllSickDay();\n\n\t\t// Sort by date (newest first)\n\t\tconst sorted = allDays.sort((a, b) =>\n\t\t\tnew Date(b.sick_date).getTime() - new Date(a.sick_date).getTime()\n\t\t);\n\n\t\tsetAllSickDays(sorted);\n\t};\n\n\tconst applyFilters = () => {\n\t\tlet filtered = [...allSickDays];\n\n\t\t// Filter by employee\n\t\tif (selectedEmployee !== \"all\") {\n\t\t\tfiltered = filtered.filter(sd => sd.user_id === selectedEmployee);\n\t\t}\n\n\t\t// Filter by date\n\t\tif (dateFilter) {\n\t\t\tfiltered = filtered.filter(sd => sd.sick_date === dateFilter);\n\t\t}\n\n\t\tsetFilteredSickDays(filtered);\n\t};\n\n\tconst getUserName = (userId: string) => {\n\t\tconst user = users.find(u => u.id === userId);\n\t\treturn user?.name || \"Unknown\";\n\t};\n\n\tconst formatDate = (dateStr: string) => {\n\t\tconst date = new Date(dateStr);\n\t\treturn date.toLocaleDateString(language === 'es' ? 'es-ES' : 'en-US', {\n\t\t\tyear: 'numeric',\n\t\t\tmonth: 'long',\n\t\t\tday: 'numeric'\n\t\t});\n\t};\n\n\tconst handleDeleteSickDay = async (sickDay: SickDayModel) => {\n\t\tif (!window.confirm(t.confirmDeleteSickDay)) {\n\t\t\treturn;\n\t\t}\n\n\t\tconst sickDayORM = SickDayORM.getInstance();\n\t\tawait sickDayORM.deleteSickDayById(sickDay.id);\n\t\tawait loadAllSickDays();\n\t};\n\n\tconst calculateTotalSickHours = () => {\n\t\treturn filteredSickDays.reduce((sum, sd) => sum + sd.hours, 0);\n\t};\n\n\treturn (\n\t\t<Card>\n\t\t\t<CardHeader>\n\t\t\t\t<div className=\"flex justify-between items-center\">\n\t\t\t\t\t<div>\n\t\t\t\t\t\t<CardTitle className=\"flex items-center gap-2\">\n\t\t\t\t\t\t\t<Heart className=\"w-5 h-5 text-red-500\" />\n\t\t\t\t\t\t\t{t.sickDayHistory}\n\t\t\t\t\t\t</CardTitle>\n\t\t\t\t\t\t<CardDescription>\n\t\t\t\t\t\t\t{t.totalSickDays}: {calculateTotalSickHours().toFixed(1)} {language === 'es' ? 'hrs' : 'hrs'}\n\t\t\t\t\t\t</CardDescription>\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\t\t\t</CardHeader>\n\t\t\t<CardContent className=\"space-y-4\">\n\t\t\t\t<div className=\"flex gap-4 flex-wrap\">\n\t\t\t\t\t<div className=\"flex-1 min-w-[200px]\">\n\t\t\t\t\t\t<Label>{t.filterByEmployee}</Label>\n\t\t\t\t\t\t<Select value={selectedEmployee} onValueChange={setSelectedEmployee}>\n\t\t\t\t\t\t\t<SelectTrigger>\n\t\t\t\t\t\t\t\t<SelectValue />\n\t\t\t\t\t\t\t</SelectTrigger>\n\t\t\t\t\t\t\t<SelectContent>\n\t\t\t\t\t\t\t\t<SelectItem value=\"all\">{t.allEmployees}</SelectItem>\n\t\t\t\t\t\t\t\t{users.filter(u => u.role === UserRole.Employee).map((u) => (\n\t\t\t\t\t\t\t\t\t<SelectItem key={u.id} value={u.id}>\n\t\t\t\t\t\t\t\t\t\t{u.name}\n\t\t\t\t\t\t\t\t\t</SelectItem>\n\t\t\t\t\t\t\t\t))}\n\t\t\t\t\t\t\t</SelectContent>\n\t\t\t\t\t\t</Select>\n\t\t\t\t\t</div>\n\n\t\t\t\t\t<div className=\"flex-1 min-w-[200px]\">\n\t\t\t\t\t\t<Label>{t.filterByDate}</Label>\n\t\t\t\t\t\t<Input\n\t\t\t\t\t\t\ttype=\"date\"\n\t\t\t\t\t\t\tvalue={dateFilter}\n\t\t\t\t\t\t\tonChange={(e) => setDateFilter(e.target.value)}\n\t\t\t\t\t\t/>\n\t\t\t\t\t</div>\n\n\t\t\t\t\t{(selectedEmployee !== \"all\" || dateFilter) && (\n\t\t\t\t\t\t<div className=\"flex items-end\">\n\t\t\t\t\t\t\t<Button\n\t\t\t\t\t\t\t\tvariant=\"outline\"\n\t\t\t\t\t\t\t\tonClick={() => {\n\t\t\t\t\t\t\t\t\tsetSelectedEmployee(\"all\");\n\t\t\t\t\t\t\t\t\tsetDateFilter(\"\");\n\t\t\t\t\t\t\t\t}}\n\t\t\t\t\t\t\t>\n\t\t\t\t\t\t\t\t{t.clearFilters}\n\t\t\t\t\t\t\t</Button>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t)}\n\t\t\t\t</div>\n\n\t\t\t\t{filteredSickDays.length === 0 ? (\n\t\t\t\t\t<p className=\"text-center text-muted-foreground py-8\">{t.noSickDays}</p>\n\t\t\t\t) : (\n\t\t\t\t\t<div className=\"overflow-x-auto\">\n\t\t\t\t\t\t<Table>\n\t\t\t\t\t\t\t<TableHeader>\n\t\t\t\t\t\t\t\t<TableRow>\n\t\t\t\t\t\t\t\t\t<TableHead>{t.employeeName}</TableHead>\n\t\t\t\t\t\t\t\t\t<TableHead>{t.sickDayDate}</TableH