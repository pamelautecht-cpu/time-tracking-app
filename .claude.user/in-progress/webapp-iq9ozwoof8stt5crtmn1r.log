er: UserModel; regularHours: number; overtimeHours: number; totalHours: number; regularPay: number; overtimePay: number; totalPay: number }[]>([]);\n\tconst [detailedTimeCards, setDetailedTimeCards] = useState<TimeCardEntryModel[]>([]);\n\n\tconst t = translations[language];\n\n\tuseEffect(() => {\n\t\t// Set default to current week\n\t\tconst today = new Date();\n\t\tconst monday = new Date(today);\n\t\tmonday.setDate(today.getDate() - today.getDay() + 1);\n\t\tsetWeekStart(monday.toISOString().slice(0, 10));\n\t}, []);\n\n\tuseEffect(() => {\n\t\tif (weekStart) {\n\t\t\tloadWeeklyReport();\n\t\t}\n\t}, [weekStart, users]);\n\n\tconst loadWeeklyReport = async () => {\n\t\tconst timeCardORM = TimeCardEntryORM.getInstance();\n\t\tconst allCards = await timeCardORM.getAllTimeCardEntry();\n\n\t\t// Normalize data from ORM\n\t\tconst normalized = normalizeTimeCardEntriesFromORM(allCards);\n\n\t\tconst weekStartDate = new Date(weekStart);\n\t\tconst weekStartTimestamp = Math.floor(weekStartDate.getTime() / 1000);\n\t\tconst weekEndTimestamp = weekStartTimestamp + 604800; // 7 days\n\n\t\tconst weekCards = normalized.filter(tc => {\n\t\t\tconst clockIn = parseInt(tc.clock_in_timestamp);\n\t\t\treturn clockIn >= weekStartTimestamp && clockIn < weekEndTimestamp && tc.total_hours;\n\t\t});\n\n\t\t// Store detailed time cards\n\t\tsetDetailedTimeCards(weekCards.sort((a, b) => parseInt(a.clock_in_timestamp) - parseInt(b.clock_in_timestamp)));\n\n\t\tconst employeeHours = users\n\t\t\t.filter(u => u.role === UserRole.Employee)\n\t\t\t.map(user => {\n\t\t\t\tconst userCards = weekCards.filter(tc => tc.user_id === user.id);\n\t\t\t\tconst totalHours = userCards.reduce((sum, tc) => sum + (tc.total_hours || 0), 0);\n\t\t\t\tconst regularHours = Math.min(totalHours, 40);\n\t\t\t\tconst overtimeHours = Math.max(totalHours - 40, 0);\n\n\t\t\t\t// Calculate pay\n\t\t\t\tconst hourlyRate = user.hourly_rate || 0;\n\t\t\t\tconst regularPay = regularHours * hourlyRate;\n\t\t\t\tconst overtimePay = overtimeHours * hourlyRate * 1.5;\n\t\t\t\tconst totalPay = regularPay + overtimePay;\n\n\t\t\t\treturn {\n\t\t\t\t\tuser,\n\t\t\t\t\tregularHours,\n\t\t\t\t\tovertimeHours,\n\t\t\t\t\ttotalHours,\n\t\t\t\t\tregularPay,\n\t\t\t\t\tovertimePay,\n\t\t\t\t\ttotalPay,\n\t\t\t\t};\n\t\t\t});\n\n\t\tsetReportData(employeeHours);\n\t};\n\n\tconst formatHours = (hours: number) => {\n\t\treturn `${hours.toFixed(2)} hrs`;\n\t};\n\n\tconst formatPay = (amount: number) => {\n\t\treturn `$${amount.toFixed(2)}`;\n\t};\n\n\tconst formatDate = (timestamp: string) => {\n\t\tconst date = new Date(parseInt(timestamp) * 1000);\n\t\treturn date.toLocaleString(language === 'es' ? 'es-ES' : 'en-US');\n\t};\n\n\tconst formatDateOnly = (timestamp: string) => {\n\t\tconst date = new Date(parseInt(timestamp) * 1000);\n\t\treturn date.toLocaleDateString(language === 'es' ? 'es-ES' : 'en-US');\n\t};\n\n\tconst getDayOfWeek = (timestamp: string) => {\n\t\tconst date = new Date(parseInt(timestamp) * 1000);\n\t\treturn date.toLocaleDateString(language === 'es' ? 'es-ES' : 'en-US', { weekday: 'long' });\n\t};\n\n\tconst getUserName = (userId: string) => {\n\t\tconst user = users.find(u => u.id === userId);\n\t\treturn user?.name || \"Unknown\";\n\t};\n\n\tconst handlePrint = () => {\n\t\twindow.print();\n\t};\n\n\tconst handleExportCSV = () => {\n\t\tif (detailedTimeCards.length === 0) return;\n\n\t\t// Create CSV header\n\t\tconst headers = [\n\t\t\tt.employeeName,\n\t\t\tt.date,\n\t\t\tt.day,\n\t\t\tt.clockInTime,\n\t\t\tt.clockOutTime,\n\t\t\tt.location,\n\t\t\tt.workDescription,\n\t\t\tt.lunchDuration,\n\t\t\tt.hours,\n\t\t\tt.hourlyRate,\n\t\t\tt.pay\n\t\t];\n\n\t\t// Create CSV rows\n\t\tconst rows = detailedTimeCards.map(tc => {\n\t\t\tconst user = users.find(u => u.id === tc.user_id);\n\t\t\tconst userName = user?.name || \"Unknown\";\n\t\t\tconst hourlyRate = user?.hourly_rate || 0;\n\t\t\tconst date = formatDateOnly(tc.clock_in_timestamp);\n\t\t\tconst day = getDayOfWeek(tc.clock_in_timestamp);\n\t\t\tconst clockIn = formatDate(tc.clock_in_timestamp);\n\t\t\tconst clockOut = tc.clock_out_timestamp ? formatDate(tc.clock_out_timestamp) : \"‚Äî\";\n\t\t\tconst location = tc.location || \"‚Äî\";\n\t\t\tconst workDescription = (tc.work_description || \"‚Äî\").replace(/\"/g, '\"\"'); // Escape quotes\n\t\t\tconst lunchDuration = tc.lunch_duration_minutes ? `${Math.round(tc.lunch_duration_minutes)} min` : \"‚Äî\";\n\t\t\tconst hours = tc.total_hours ? `${tc.total_hours.toFixed(2)} hrs` : \"‚Äî\";\n\t\t\tconst pay = tc.total_hours ? formatPay(tc.total_hours * hourlyRate) : \"‚Äî\";\n\n\t\t\treturn [\n\t\t\t\t`\"${userName}\"`,\n\t\t\t\t`\"${date}\"`,\n\t\t\t\t`\"${day}\"`,\n\t\t\t\t`\"${clockIn}\"`,\n\t\t\t\t`\"${clockOut}\"`,\n\t\t\t\t`\"${location}\"`,\n\t\t\t\t`\"${workDescription}\"`,\n\t\t\t\t`\"${lunchDuration}\"`,\n\t\t\t\t`\"${hours}\"`,\n\t\t\t\t`\"${formatPay(hourlyRate)}\"`,\n\t\t\t\t`\"${pay}\"`\n\t\t\t].join(',');\n\t\t});\n\n\t\t// Combine header and rows\n\t\tconst csv = [headers.join(','), ...rows].join('\\n');\n\n\t\t// Create blob and download\n\t\tconst blob = new Blob([csv], { type: 'text/csv;charset=utf-8;' });\n\t\tconst link = document.createElement('a');\n\t\tconst url = URL.createObjectURL(blob);\n\t\tlink.setAttribute('href', url);\n\t\tlink.setAttribute('download', `weekly-report-${weekStart}.csv`);\n\t\tlink.style.visibility = 'hidden';\n\t\tdocument.body.appendChild(link);\n\t\tlink.click();\n\t\tdocument.body.removeChild(link);\n\t};\n\n\treturn (\n\t\t<div className=\"space-y-4\">\n\t\t\t<Card>\n\t\t\t\t<CardHeader>\n\t\t\t\t\t<div className=\"flex justify-between items-center\">\n\t\t\t\t\t\t<div>\n\t\t\t\t\t\t\t<CardTitle>{t.weeklyReport}</CardTitle>\n\t\t\t\t\t\t\t<CardDescription>\n\t\t\t\t\t\t\t\tOvertime calculated for hours worked beyond 40 hours/week\n\t\t\t\t\t\t\t\t<br />\n\t\t\t\t\t\t\t\t<span className=\"text-xs text-muted-foreground\">\n\t\t\t\t\t\t\t\t\tNote: Sick days are tracked separately in the \"Sic