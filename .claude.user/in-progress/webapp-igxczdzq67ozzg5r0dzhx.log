n\t\t);\n\t}\n\n\t// Safety check - should not happen but prevents TypeScript errors\n\tif (!currentUser) {\n\t\treturn null;\n\t}\n\n\treturn (\n\t\t<div className=\"min-h-screen bg-gradient-to-br from-blue-50 to-indigo-100 dark:from-gray-900 dark:to-gray-800\">\n\t\t\t<div className=\"container mx-auto p-6 space-y-6\">\n\t\t\t\t<div className=\"flex justify-between items-center\">\n\t\t\t\t\t<div>\n\t\t\t\t\t\t<h1 className=\"text-3xl font-bold\">\n\t\t\t\t\t\t\t{employeeOnlyMode ? t.myTimeTracking : t.timeTrackingSystem}\n\t\t\t\t\t\t</h1>\n\t\t\t\t\t\t<p className=\"text-muted-foreground\">\n\t\t\t\t\t\t\t{t.welcome}, {currentUser.name} ({currentUser.role === UserRole.Manager ? t.manager : t.employee})\n\t\t\t\t\t\t</p>\n\t\t\t\t\t</div>\n\t\t\t\t\t<div className=\"flex gap-2\">\n\t\t\t\t\t\t<Button onClick={toggleLanguage} variant=\"outline\" size=\"sm\">\n\t\t\t\t\t\t\t<Languages className=\"w-4 h-4 mr-2\" />\n\t\t\t\t\t\t\t{language === 'en' ? 'Español' : 'English'}\n\t\t\t\t\t\t</Button>\n\t\t\t\t\t\t{!employeeOnlyMode && (\n\t\t\t\t\t\t\t<Button onClick={handleLogout} variant=\"outline\">\n\t\t\t\t\t\t\t\t{t.switchUser}\n\t\t\t\t\t\t\t</Button>\n\t\t\t\t\t\t)}\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\n\t\t\t\t{currentUser.role === UserRole.Employee ? (\n\t\t\t\t\t<EmployeeView user={currentUser} language={language} />\n\t\t\t\t) : (\n\t\t\t\t\t<ManagerView user={currentUser} users={users} language={language} onUsersChange={loadUsers} />\n\t\t\t\t)}\n\t\t\t</div>\n\t\t</div>\n\t);\n}\n\nfunction SickDayCard({ user, language }: { user: UserModel; language: Language }) {\n\tconst [sickDays, setSickDays] = useState<SickDayModel[]>([]);\n\tconst [showAddDialog, setShowAddDialog] = useState(false);\n\tconst [sickDate, setSickDate] = useState(\"\");\n\tconst [sickHours, setSickHours] = useState(\"8\");\n\tconst [sickNotes, setSickNotes] = useState(\"\");\n\n\tconst t = translations[language];\n\n\tuseEffect(() => {\n\t\tloadSickDays();\n\t}, [user.id]);\n\n\tconst loadSickDays = async () => {\n\t\tconst sickDayORM = SickDayORM.getInstance();\n\t\tconst userSickDays = await sickDayORM.getSickDayByUserId(user.id);\n\n\t\t// Sort by date (newest first)\n\t\tconst sorted = userSickDays.sort((a, b) =>\n\t\t\tnew Date(b.sick_date).getTime() - new Date(a.sick_date).getTime()\n\t\t);\n\n\t\tsetSickDays(sorted);\n\t};\n\n\tconst handleAddSickDay = async () => {\n\t\tif (!sickDate || !sickHours) {\n\t\t\talert(\"Please fill in date and hours\");\n\t\t\treturn;\n\t\t}\n\n\t\tconst sickDayORM = SickDayORM.getInstance();\n\t\tawait sickDayORM.insertSickDay([{\n\t\t\tuser_id: user.id,\n\t\t\tsick_date: sickDate,\n\t\t\thours: parseFloat(sickHours),\n\t\t\tnotes: sickNotes || null,\n\t\t} as SickDayModel]);\n\n\t\t// Reset form\n\t\tsetSickDate(\"\");\n\t\tsetSickHours(\"8\");\n\t\tsetSickNotes(\"\");\n\t\tsetShowAddDialog(false);\n\n\t\tawait loadSickDays();\n\t};\n\n\tconst handleDeleteSickDay = async (sickDay: SickDayModel) => {\n\t\tif (!window.confirm(t.confirmDeleteSickDay)) {\n\t\t\treturn;\n\t\t}\n\n\t\tconst sickDayORM = SickDayORM.getInstance();\n\t\tawait sickDayORM.deleteSickDayById(sickDay.id);\n\t\tawait loadSickDays();\n\t};\n\n\tconst formatDate = (dateStr: string) => {\n\t\tconst date = new Date(dateStr);\n\t\treturn date.toLocaleDateString(language === 'es' ? 'es-ES' : 'en-US', {\n\t\t\tyear: 'numeric',\n\t\t\tmonth: 'long',\n\t\t\tday: 'numeric'\n\t\t});\n\t};\n\n\tconst totalSickHours = sickDays.reduce((sum, sd) => sum + sd.hours, 0);\n\n\treturn (\n\t\t<>\n\t\t\t<Card>\n\t\t\t\t<CardHeader>\n\t\t\t\t\t<div className=\"flex justify-between items-center\">\n\t\t\t\t\t\t<div>\n\t\t\t\t\t\t\t<CardTitle className=\"flex items-center gap-2\">\n\t\t\t\t\t\t\t\t<Heart className=\"w-5 h-5 text-red-500\" />\n\t\t\t\t\t\t\t\t{t.mySickDays}\n\t\t\t\t\t\t\t</CardTitle>\n\t\t\t\t\t\t\t<CardDescription>\n\t\t\t\t\t\t\t\t{t.totalSickDays}: {totalSickHours.toFixed(1)} {language === 'es' ? 'hrs' : 'hrs'}\n\t\t\t\t\t\t\t</CardDescription>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t\t<Button onClick={() => setShowAddDialog(true)} size=\"sm\">\n\t\t\t\t\t\t\t<Plus className=\"w-4 h-4 mr-2\" />\n\t\t\t\t\t\t\t{t.addSickDay}\n\t\t\t\t\t\t</Button>\n\t\t\t\t\t</div>\n\t\t\t\t</CardHeader>\n\t\t\t\t<CardContent>\n\t\t\t\t\t{sickDays.length === 0 ? (\n\t\t\t\t\t\t<p className=\"text-center text-muted-foreground py-8\">{t.noSickDays}</p>\n\t\t\t\t\t) : (\n\t\t\t\t\t\t<div className=\"overflow-x-auto\">\n\t\t\t\t\t\t\t<Table>\n\t\t\t\t\t\t\t\t<TableHeader>\n\t\t\t\t\t\t\t\t\t<TableRow>\n\t\t\t\t\t\t\t\t\t\t<TableHead>{t.sickDayDate}</TableHead>\n\t\t\t\t\t\t\t\t\t\t<TableHead>{t.sickDayHours}</TableHead>\n\t\t\t\t\t\t\t\t\t\t<TableHead>{t.sickDayNotes}</TableHead>\n\t\t\t\t\t\t\t\t\t\t<TableHead>{t.actions}</TableHead>\n\t\t\t\t\t\t\t\t\t</TableRow>\n\t\t\t\t\t\t\t\t</TableHeader>\n\t\t\t\t\t\t\t\t<TableBody>\n\t\t\t\t\t\t\t\t\t{sickDays.map((sd) => (\n\t\t\t\t\t\t\t\t\t\t<TableRow key={sd.id}>\n\t\t\t\t\t\t\t\t\t\t\t<TableCell className=\"font-medium whitespace-nowrap\">\n\t\t\t\t\t\t\t\t\t\t\t\t{formatDate(sd.sick_date)}\n\t\t\t\t\t\t\t\t\t\t\t</TableCell>\n\t\t\t\t\t\t\t\t\t\t\t<TableCell>{sd.hours} hrs</TableCell>\n\t\t\t\t\t\t\t\t\t\t\t<TableCell className=\"max-w-xs truncate\">{sd.notes || \"—\"}</TableCell>\n\t\t\t\t\t\t\t\t\t\t\t<TableCell>\n\t\t\t\t\t\t\t\t\t\t\t\t<Button\n\t\t\t\t\t\t\t\t\t\t\t\t\tvariant=\"outline\"\n\t\t\t\t\t\t\t\t\t\t\t\t\tsize=\"sm\"\n\t\t\t\t\t\t\t\t\t\t\t\t\tonClick={() => handleDeleteSickDay(sd)}\n\t\t\t\t\t\t\t\t\t\t\t\t\tclassName=\"text-red-600 hover:text-red-700 hover:bg-red-50\"\n\t\t\t\t\t\t\t\t\t\t\t\t>\n\t\t\t\t\t\t\t\t\t\t\t\t\t<Trash2 className=\"w-4 h-4\" />\n\t\t\t\t\t\t\t\t\t\t\t\t</Button>\n\t\t\t\t\t\t\t\t\t\t\t</TableCell>\n\t\t\t\t\t\t\t\t\t\t</TableRow>\n\t\t\t\t\t\t\t\t\t))}\n\t\t\t\t\t\t\t\t</TableBody>\n\t\t\t\t\t\t\t</Table>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t)}\n\t\t\t\t</CardContent>\n\t\t\t</Card>\n\n\t\t\t<Dialog open={showAddDialog} onOpenChange={setShowAddDialog}>\n\t\t\t\t<DialogContent>\n\t\t\t\t\t<DialogHeader>\n\t\t\t\t\t\t<DialogTitle>{t.addSickDay}</DialogTitle>\n\t\t\t\t\t</DialogHeader>\n\t\t\t\t\t<div className=\"space-y-4\">\n\t\t\t\t\t\t<div>\n\t\t\t\t\t\t\t<Label>{t.sickDayDate}</Label>\n\t\t\t\t\t\t\t<Input\n\t\t\t\t\t\t\t\ttype=\"date\"\n\t\t\t\t\t\t\t\tvalue={sickDate}\n\t\t\t\t\t\t\t\tonChange={(e) => setSickDate(e.target.value)}\n\t\t\t\t\t\t\t\tmax={new Date().toISOString().split('T')[0]}\n\t\t\t\t\t\t\t/>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t\t<div>\n\t\t\t\t\t\t\t<Label>{t.sickDayHours}</Label>\n\t\t\t\t\t\t\t<Input\n\t\t\t\t\t\t\t\ttype=\"number\"\n\t\t\t\t\t\t\t\tmin=\"0.5\"\n\t\t\t\t\t\t\t\tmax=\"24\"\n\t\t\t\t\t\t\t\tstep=\"0.5\"\n\t\t\t\t\t\t\t\tvalue={sickHours}\n\t\t\t\t\t\t\t\tonChange={(e) => setSickHours(e.target.value)}\n\t\t\t\t\t\t\t/>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t\t<div>\n\t\t\t\t\t\t\t<Label>{t.sickDayNotes}</Label>\n\t\t\t\t\t\t\t<Textarea\n\t\t\t\t\t\t\t\tvalue={sickNotes}\n\t\t\t\t\t\t\t\tonChange={(e) => setSickNotes(e.target.value)}\n\t\t\t\t\t\t\t\tplaceholder={t.sickDayNotesPlaceholder}\n\t\t\t\t\t\t\t\trows={3}\n\t\t\t\t\t\t\t/>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t</div>\n\t\t\t\t\t<DialogFooter>\n\t\t\t\t\t\t<Button variant=\"outline\" onClick={() => setShowAddDialog(false)}>\n\t\t\t\t\t\t\t{t.cancel}\n\t\t\t\t\t\t</Button>\n\t\t\t\t\t\t<Button onClick={handleAddSickDay} disabled={!sickDate || !sickHours}>\n\t\t\t\t\t\t\t{t.saveChanges}\n\t\t\t\t\t\t</Button>\n\t\t\t\t\t</DialogFooter>\n\t\t\t\t</DialogContent>\n\t\t\t</Dialog>\n\t\t</>\n\t);\n}\n\nfunction EmployeeView({ user, language }: { user: UserModel; language: Language }) {\n\tconst [timeCards, setTimeCards] = useState<TimeCardEntryModel[]>([]);\n\tconst [activeShift, setActiveShift] = useState<TimeCardEntryModel | null>(null);\n\tconst [elapsedTime, setElapsedTime] = useState(0);\n\tconst [location, setLocation] = useState(\"\");\n\tconst [workDescription, setWorkDescription] = useState(\"\");\n\tconst [isOnLunch, setIsOnLunch] = use